<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/imageView24"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_margin="15dp"
        android:background="@drawable/ic_touxiang" />

    <TextView
        android:id="@+id/textView79"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageView24"
        android:layout_marginTop="10dp"
        android:layout_toEndOf="@+id/imageView24"
        android:text="@string/customerexample" />

    <TextView
        android:id="@+id/textView80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView24"
        android:layout_toEndOf="@+id/imageView24"
        android:text="@string/evaluateexample" />

    <view
        android:id="@+id/view2"
        class="com.lx.car.view.RatingBarView"
        android:layout_width="wrap_content"
        android:layout_height="15dp"
        android:layout_marginLeft="20dp"
        android:columnCount="5"
        app:starEmpty="@drawable/ic_xingxing"
        app:starFill="@drawable/ic_xingxing_yellow"
        android:layout_alignTop="@+id/textView79"
        android:layout_toEndOf="@+id/textView79"
        android:layout_marginStart="30dp" />
</RelativeLayout>